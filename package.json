{
  "name": "@mcpjam/inspector-vite",
  "version": "1.0.0",
  "type": "module",
  "description": "MCPJam Inspector",
  "license": "Apache-2.0",
  "author": "MCPJam (https://mcpjam.com)",
  "homepage": "https://mcpjam.com",
  "bugs": "https://github.com/mcpjam/inspector/issues",
  "bin": {
    "inspector-vite": "bin/start.js"
  },
  "files": [
    "bin",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "start": "NODE_ENV=production node bin/start.js",
    "publish": "npm run build && npm publish --access public",
    "prettier-fix": "prettier --write .",
    "install:deps": "cd server && npm install && cd ../client && npm install",
    "docker:build": "docker build -t mcpjam/mcp-inspector:latest .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t mcpjam/mcp-inspector:dev .",
    "docker:run": "docker run -p 3001:3001 mcpjam/mcp-inspector:latest",
    "docker:run:dev": "docker run -p 3001:3001 -p 5173:5173 mcpjam/mcp-inspector:dev",
    "docker:up": "docker-compose up -d",
    "docker:up:dev": "docker-compose --profile dev up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi all"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.23",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hono/node-server": "^1.13.7",
    "@hookform/resolvers": "^3.10.0",
    "@mastra/core": "^0.10.6",
    "@mastra/mcp": "^0.10.6",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "@uiw/react-json-view": "^2.0.0-alpha.33",
    "ai": "^4.3.19",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^12.23.6",
    "hono": "^4.6.11",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "ollama-ai-provider": "^1.2.0",
    "postcss": "^8.5.6",
    "radix-ui": "^1.4.2",
    "react": "19.1.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.3",
    "react18-json-view": "^0.2.9",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "simple-icons": "^15.6.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "tailwindcss": "^4.1.11",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "vite": "^6.0.7"
  }
}
